itLine: {\n                            show: true // 不显示网格线\n                        },\n                    },\n                    series: [\n                        {type: 'bar'},\n                        {type: 'bar'},\n                        {type: 'bar'}\n                    ]\n                }\n\n                // 支付类型\n                state.payTypeOPtion = {\n                    title: {},\n                    tooltip: {\n                        trigger: 'item',\n                        formatter: '{a} <br/>{b} : {c}￥ ({d}%)'\n                    },\n                    color: ['#dc3545', '#007bff', '#28a745'],\n                    series: [\n                        {\n                            name: '支付渠道',\n                            type: 'pie',\n                            radius: '90%',\n                            avoidLabelOverlap: false,\n                            roseType: 'radius',\n                            center: ['50%', '50%'],\n                            itemStyle:{\n                                borderRadius: 10,\n                                borderColor: '#fff',\n                                borderWidth: 2\n                            },\n                            label:{\n                                show: true,\n                                position: 'inner'\n                            },\n                            labelLine: {\n                                show:false\n                            },\n                            data: [\n                                {value: 335, name: '微信'},\n                                {value: 310, name: '支付宝'},\n                                {value: 274, name: '银联'}\n                            ].sort(function (a, b) {\n                                return a.value - b.value;\n                            })\n                        }\n                    ]\n                }\n\n                // 客户满意度\n                state.satisfactions = [\n                   